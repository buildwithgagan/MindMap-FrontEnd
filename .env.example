# API Configuration
# Copy this file to .env.local for local development
# Or use .env.development for development and .env.production for production

# API Base URL - Used for REST API calls
NEXT_PUBLIC_API_BASE_URL=http://localhost:3000

# WebSocket Base URL - Used for Socket.io connections
# If not set, will fall back to NEXT_PUBLIC_API_BASE_URL
NEXT_PUBLIC_WS_BASE_URL=
